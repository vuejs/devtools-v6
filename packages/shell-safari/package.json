{
  "name": "@vue-devtools/shell-safari",
  "version": "0.0.0",
  "scripts": {
    "build": "rimraf ./build && cross-env NODE_ENV=production webpack --progress",
    "archive": "cd ./app && xcodebuild -scheme VueDevTools -archivePath ./archive archive",
    "extension": "cd ./app && xcodebuild -exportArchive -archivePath ./archive.xcarchive -exportPath ./extension -exportOptionsPlist ./exportOptions.plist",
    "safari": "yarn archive && yarn extension"
  },
  "dependencies": {
    "@vue-devtools/app-backend-core": "^0.0.0",
    "@vue-devtools/app-frontend": "^0.0.0",
    "@vue-devtools/shared-utils": "^0.0.0"
  },
  "devDependencies": {
    "@vue-devtools/build-tools": "^0.0.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  }
}
